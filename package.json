{
  "name": "client-to-code",
  "version": "1.0.0",
  "description": "Development environment Click on the component to jump to the corresponding file",
  "scripts": {
    "build": "pnpm run -r --filter=./packages/{middleware,vite-plugin,react-client} build",
    "dev": "pnpm run -r --filter=./packages/{middleware,vite-plugin,react-client} dev",
    "play:react-vite-demo": "pnpm run -r --filter=./packages/react-vite-demo dev"
  },
  "exports": {
    "./vite-plugin-react": "./packages/vite-plugin/lib/index.js",
    "./vite-plugin-react/types": "./packages/vite-plugin/lib/index.d.ts",
    "./react-client": {
      "import": "./packages/react-client/lib/dist/index.esm.js",
      "require": "./packages/react-client/lib/dist/index.cjs.js",
      "types": "./packages/react-client/lib/types/index.d.ts"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "tubowen",
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.14.0",
    "eslint": "^8.34.0",
    "lint-staged": "^13.1.2",
    "react-dev-utils": "^12.0.1",
    "rollup": "^3.17.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1"
  }
}
